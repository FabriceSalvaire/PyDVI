#version 330

/* *********************************************************************************************** */

#include(../include/rule_shader_program_interface.glsl)

/* *********************************************************************************************** */

out VertexAttributesIn
{
  vec2 position;
  vec2 dimension;
  vec4 colour;
} vertexIn;

/* *********************************************************************************************** */

